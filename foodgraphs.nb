splittotal = StringSplit[strtotal, "\[Bullet]"];

ss2 = StringTrim[#] & /@ (StringSplit[splittotal, 
          
     "pairs well with:" | "pair well with:" | "pears well with:"]);

ss3 = ToLowerCase[({#[[1]], 
              Flatten[StringSplit[
                  StringTrim[StringSplit[#[[2]], "," | " and "], "."], 
                  ","]]} & /@ ss2)];

words = ss3 // Flatten // Union;

editdistances = 
    Table[
   EditDistance[words[[m]], words[[n]]], {m, Length[words]}, {n, 
        Length[words]}];

editdistance1or2 = #[[2]] -> #[[1]] & /@ 
      Union[Sort[{words[[#[[1]]]], words[[#[[2]]]]}] & /@ 
          Position[editdistances, 1 | 2]];

pluralreps = 
    Select[editdistance1or2, 
      If[StringLength[#[[1]]] > 3 && StringLength[#[[2]]] > 3, 
          StringTake[#[[1]], ;; 4] == StringTake[#[[2]], ;; 4], 
          StringTake[#[[1]], ;; -2] == StringTake[#[[2]], ;; -2]] &];

pluralreps = 
    Join[{"cherries" -> "cherry", 
    "cilantro (coriander)" -> "cilantro", 
        "citrus fruit" -> "citrus", "cranberries" -> "cranberry", 
        "curries" -> "curry", "juniper berry" -> "juniper", 
        "star anise" -> "anise", "sun-dried" -> "sun-dried tomato", 
        "currants,black and red," -> "currants"}, pluralreps];

gotogether = 
    Flatten[
      Table[{ss3[[n, 1]], #} & /@ ss3[[n, 2]], {n, Length[ss3]}], 
            1] /. pluralreps // Sort // Union;

graphedges1 = #[[1]] <-> #[[2]] & /@ gotogether;

graphedges2 = 
    Union[DeleteCases[graphedges1, 
        "basil" <-> "sweet basil is the best basil for pesto" | 
          "celery seed" <-> "cinnamon coriander" | n_ <-> n_ | 
          n_ <-> "tomato salads"]];

Graph[RandomSample[graphedges2, 100], 
  VertexLabels -> "Name", ImageSize -> 1000, ImagePadding -> 100]

